{
    "workbench.startupEditor": "newUntitledFile",
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.historyExplorer.enabled": true,
    "explorer.confirmDragAndDrop": false,
    "workbench.statusBar.feedback.visible": false,
    "editor.fontFamily": "'JetBrains Mono', 'Fira Code', Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "crystal-ide.backend": "custom",
    "crystal-ide.customCommand": "/usr/local/bin/scry",
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "workbench.colorTheme": "Remedy - Dark (Straight)",
    "editor.cursorStyle": "block",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.minimap.enabled": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "python.dataScience.askForKernelRestart": false,
    "git.suggestSmartCommit": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "crystal-lang.server": "/usr/local/bin/scry",
    "crystal-lang.completion": true,
    "crystal-lang.hover": true,
    "crystal-lang.implementations": true,
    "crystal-lang.shards": "/usr/local/bin/shards",
    "swift.languageServerPath": "/usr/local/bin/sourcekite",
    "swift.path.sourcekite": "/usr/local/bin/sourcekite",
    "[glsl]": {},
    "liveServer.settings.donotVerifyTags": true
}